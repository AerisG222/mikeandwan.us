@model MawMvcApp.ViewModels.Tools.Dotnet.PathsModel
@{ 
    Layout = "_LayoutSidebar";
    ViewBag.Title = "tools | .net path methods"; 
}

@section header {
    @Html.Partial("_PageHeader", "Path Related API Examples")
}

@section sidebar {
    @Html.Partial("_ToolSidebar", new SidebarModel() { Group = "dotnet", Item = "paths" })
}

<div class="table-responsive">
<table class="table table-striped table-hover table-condensed">
	<thead>
		<tr>
	        <th>API Call</th>
	        <th>Result</th>
	    </tr>
    </thead>
    <tbody>
    
	@foreach(var item in Model.Paths) {
		<tr>
			<td>@item[0]</td>
			<td>@item[1]</td>
		</tr>	
	}
	
	</tbody>
</table>
</div>   