{
  "name": "client-apps-ng",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-learning-modernizr": "npx modernizr -c projects/learning/modernizr-config.json -d projects/learning/src",
    "build-dev:binary-clock": "ng build binary-clock && ng lint binary-clock",
    "build-dev:googlemaps": "ng build googlemaps && ng lint googlemaps",
    "build-dev:learning": "npm run build-learning-modernizr & ng build learning && ng lint learning",
    "build-dev:memory": "ng build memory & ng lint memory",
    "build-dev:money-spin": "ng build money-spin && ng lint money-spin",
    "build-dev:weekend-countdown": "ng build weekend-countdown && ng lint weekend-countdown",
    "build-dev": "npm run build-dev:binary-clock && npm run build-dev:googlemaps && npm run build-dev:learning && npm run build-dev:memory && npm run build-dev:money-spin && npm run build-dev:weekend-countdown",
    "build-prod:binary-clock": "ng build binary-clock --configuration production",
    "build-prod:googlemaps": "ng build googlemaps --configuration production",
    "build-prod:learning": "npm run build-learning-modernizr & ng build learning --configuration production",
    "build-prod:memory": "ng build memory --configuration production",
    "build-prod:money-spin": "ng build money-spin --configuration production",
    "build-prod:weekend-countdown": "ng build weekend-countdown --configuration production",
    "build-prod": "npm run build-prod:binary-clock && npm run build-prod:googlemaps && npm run build-prod:learning && npm run build-prod:memory && npm run build-prod:money-spin && npm run build-prod:weekend-countdown"
  },
  "private": true,
  "dependencies": {
