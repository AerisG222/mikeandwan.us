{
    "authors": [ "Mike Morano <mmorano@mikeandwan.us>" ],
    "description": "Entity Framework data access classes for www.mikeandwan.us",
    "version": "1.0.0-beta1",
    "buildOptions": {
        "debugType": "portable",
        "warningsAsErrors": true
    },
    "dependencies": {
        "Microsoft.EntityFrameworkCore": "1.0.0-rc2-final",
        "Microsoft.EntityFrameworkCore.Tools": {
            "type": "build",
            "version": "1.0.0-preview1-final"
        },
        "Microsoft.EntityFrameworkCore.Relational": "1.0.0-rc2-final",
        "Microsoft.EntityFrameworkCore.Relational.Design": "1.0.0-rc2-final",
        "Microsoft.Extensions.DependencyInjection": "1.0.0-rc2-final",
        "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0-rc2-final",
        "Npgsql.EntityFrameworkCore.PostgreSQL": "1.0.0-rc2-release1",
        "Npgsql.EntityFrameworkCore.PostgreSQL.Design": "1.0.0-rc2-release1"
    },
    "frameworks": {
        "net451": { },
        "netstandard1.5": {
            "imports": [ 
                "dotnet55", 
                "portable-dnxcore50+net45+win8+wp8+wpa81",
                "portable-net45+win8+wp8",
                "netcoreapp1.0"
            ]
	    }
    },
    "tools": {
        "Microsoft.EntityFrameworkCore.Tools": {
            "version": "1.0.0-preview1-final",
            "imports": [
                "portable-net45+win8+dnxcore50",
                "portable-net45+win8"
            ]
        }
    }
}
